{
  "[javascript]": {
    "editor.hover.enabled": false
  },
  "[javascriptreact]": {
    "editor.hover.enabled": false
  },
  "[typescript]": {
    "editor.hover.enabled": false
  },
  "[typescriptreact]": {
    "editor.hover.enabled": false
  },

  "highlight.include": ["**/csv/Arvore_do_projeto.csv"],
  "highlight.regexes": {
    "\"(?:[^\"]|\"\")*\"": {
      "decorations": [
        { "color": "#1f2937", "backgroundColor": "#fde68a", "fontWeight": "bold" }
      ]
    },
    "\\([^()]*\\)": {
      "decorations": [
        { "color": "#064e3b", "backgroundColor": "#a7f3d0", "fontWeight": "bold" }
      ]
    },
    "\\[[^\\[\\]]*\\]": {
      "decorations": [
        { "color": "#1e3a8a", "backgroundColor": "#bfdbfe", "fontWeight": "bold" }
      ]
    }
  }
}
